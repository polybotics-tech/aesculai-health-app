{
  "expo": {
    "name": "Aesculai Health App",
    "slug": "aesculai-health-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.aesculai",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#fafeff",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#fafeff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fafeff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app wants to access your photos to allow upload for analysis",
          "cameraPermission": "The app wants to access your camera to allow upload profile photo"
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
