{
  "expo": {
    "name": "Aesculai Health",
    "slug": "aesculai-health-app",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/google-app-icon.png",
    "scheme": "com.aesculai",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#fafeff",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aesculai.healthapp",
      "infoPlist": {
        "NSCameraUsageDescription": "Aesculai would like to get access to your camera to take photos for your profile or posts",
        "NSPhotoLibraryAddUsageDescription": "Aesculai would like to access images to your device for upload",
        "NSPhotoLibraryUsageDescription": "Aesculai would like to access images to your device for upload",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1098437746889-2bdl76f5na8dqh5epvmbnkfi3aettc91"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/google-app-icon.png",
        "backgroundColor": "#fafeff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.aesculai.healthapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fafeff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app wants to access your photos to allow upload for analysis",
          "cameraPermission": "The app wants to access your camera to allow upload profile photo"
        }
      ],
      "expo-asset",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7f6867cf-ad9e-4bc8-8f5c-52d66ba82b92"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/7f6867cf-ad9e-4bc8-8f5c-52d66ba82b92"
    }
  }
}
